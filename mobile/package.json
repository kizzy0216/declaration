{
  "name": "declaration-mobile",
  "version": "0.3.11",
  "main": "node_modules/expo/AppEntry.js",
  "description": "Declaration Mobile app",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest --watchAll",
    "storybook": "start-storybook -p 7007",
    "nuke-and-pave": "rm -rf node_modules && npm cache clear --force && watchman watch-del-all && rm -rf $TMPDIR/metro* && rm -rf $TMPDIR/react-native* && npm install",
    "versionPatch": "npm version --no-git-tag-version patch",
    "publish": "npm run versionPatch && BUILD_ENVIRONMENT_MODE='stage' expo publish",
    "watch": "npx urql-devtools"
  },
  "jest": {
    "preset": "jest-expo"
  },
